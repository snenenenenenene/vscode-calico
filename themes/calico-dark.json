{
    "$schema": "vscode://schemas/color-theme",
    "name": "Calico Dark",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "operatorOverload": "#e07e3d",
        "memberOperatorOverload": "#e07e3d",
        "variable.defaultLibrary:javascript": "#ebe5d5",
        "property.defaultLibrary:javascript": "#ebe5d5",
        "variable.defaultLibrary:javascriptreact": "#ebe5d5",
        "property.defaultLibrary:javascriptreact": "#ebe5d5",
        "class:typescript": "#fcfcf0",
        "interface:typescript": "#fcfcf0",
        "enum:typescript": "#ebe5d5",
        "enumMember:typescript": "#e07e3d",
        "namespace:typescript": "#ebe5d5",
        "variable.defaultLibrary:typescript": "#ebe5d5",
        "property.defaultLibrary:typescript": "#ebe5d5",
        "class:typescriptreact": "#fcfcf0",
        "interface:typescriptreact": "#fcfcf0",
        "enum:typescriptreact": "#ebe5d5",
        "enumMember:typescriptreact": "#e07e3d",
        "namespace:typescriptreact": "#ebe5d5",
        "variable.defaultLibrary:typescriptreact": "#ebe5d5",
        "property.defaultLibrary:typescriptreact": "#ebe5d5",
        "intrinsic:python": "#ebe5d5",
        "module:python": "#e07e3d",
        "class:python": "#fcfcf0",
        "macro:rust": "#fcfcf0",
        "namespace:rust": "#ebe5d5",
        "selfKeyword:rust": "#ebe5d5"
    },
    "colors": {
        "foreground": "#E8D4C0",
        "editor.foreground": "#E8D4C0",
        "editor.background": "#2D2D2D",
        
        "activityBar.background": "#2D2D2D",
        "activityBar.foreground": "#E8D4C0",
        "activityBar.inactiveForeground": "#8B8178",
        "activityBarBadge.background": "#D98E7E",
        "activityBarBadge.foreground": "#2D2D2D",
        "activityBar.border": "#1A1A1A",
        
        "sideBar.background": "#2D2D2D",
        "sideBar.foreground": "#E8D4C0",
        "sideBarTitle.foreground": "#E8D4C0",
        "sideBarSectionHeader.background": "#383432",
        "sideBarSectionHeader.foreground": "#E8D4C0",
        "sideBarSectionHeader.border": "#1A1A1A",
        "sideBar.border": "#1A1A1A",
        
        "editorGroupHeader.tabsBackground": "#2D2D2D",
        "tab.activeBackground": "#383432",
        "tab.inactiveBackground": "#2D2D2D",
        "tab.activeForeground": "#E8D4C0",
        "tab.inactiveForeground": "#8B8178",
        "tab.border": "#1A1A1A",
        "tab.activeBorderTop": "#D98E7E",
        "tab.activeModifiedBorder": "#D98E7E",
        "tab.hoverBackground": "#383432",
        "tab.hoverBorder": "#D98E7E",
        
        "editor.selectionBackground": "#5C4A4180",
        "editor.selectionHighlightBackground": "#5C4A4140",
        "editor.wordHighlightBackground": "#5C4A4130",
        "editor.wordHighlightStrongBackground": "#5C4A4150",
        "editor.lineHighlightBackground": "#38343280",
        "editor.lineHighlightBorder": "#1A1A1A",
        
        "statusBar.background": "#D98E7E",
        "statusBar.foreground": "#2D2D2D",
        "statusBar.noFolderBackground": "#5C4A41",
        "statusBar.debuggingBackground": "#A6755E",
        "statusBar.border": "#1A1A1A",
        
        "input.background": "#383432",
        "input.border": "#1A1A1A",
        "input.foreground": "#E8D4C0",
        "input.placeholderForeground": "#8B8178",
        "inputValidation.infoBackground": "#5C4A41",
        "inputValidation.infoBorder": "#D98E7E",
        
        "button.background": "#D98E7E",
        "button.foreground": "#2D2D2D",
        "button.hoverBackground": "#A6755E",
        "button.secondaryBackground": "#5C4A41",
        "button.secondaryForeground": "#E8D4C0",
        "button.secondaryHoverBackground": "#725548",
        
        "list.activeSelectionBackground": "#5C4A4180",
        "list.activeSelectionForeground": "#E8D4C0",
        "list.hoverBackground": "#38343280",
        "list.inactiveSelectionBackground": "#38343260",
        "list.dropBackground": "#5C4A4140",
        "list.highlightForeground": "#D98E7E",
        
        "scrollbar.shadow": "#1A1A1A",
        "scrollbarSlider.activeBackground": "#D98E7E80",
        "scrollbarSlider.background": "#5C4A4160",
        "scrollbarSlider.hoverBackground": "#5C4A4180",
        
        "terminal.foreground": "#E8D4C0",
        "terminal.background": "#2D2D2D",
        "terminal.ansiBlack": "#2D2D2D",
        "terminal.ansiRed": "#D98E7E",
        "terminal.ansiGreen": "#A89984",
        "terminal.ansiYellow": "#E8B4A2",
        "terminal.ansiBlue": "#8B8178",
        "terminal.ansiMagenta": "#C49B8A",
        "terminal.ansiCyan": "#B4A497",
        "terminal.ansiWhite": "#E8D4C0",
        "terminal.ansiBrightBlack": "#5C5C5C",
        "terminal.ansiBrightRed": "#E8B4A2",
        "terminal.ansiBrightGreen": "#C5B8A8",
        "terminal.ansiBrightYellow": "#F5E6D3",
        "terminal.ansiBrightBlue": "#A89984",
        "terminal.ansiBrightMagenta": "#D4BEB2",
        "terminal.ansiBrightCyan": "#C5B8A8",
        "terminal.ansiBrightWhite": "#F9F5F0",
        
        "panel.border": "#1A1A1A",
        "panelTitle.activeBorder": "#D98E7E",
        "panelTitle.activeForeground": "#E8D4C0",
        "panelTitle.inactiveForeground": "#8B8178",
        "titleBar.activeBackground": "#2D2D2D",
        "titleBar.activeForeground": "#E8D4C0",
        "titleBar.inactiveBackground": "#2D2D2D",
        "titleBar.inactiveForeground": "#8B8178",
        "titleBar.border": "#1A1A1A",
        
        "widget.shadow": "#1A1A1A80",
        "editorWidget.background": "#383432",
        "editorWidget.border": "#1A1A1A",
        "editorHoverWidget.background": "#383432",
        "editorHoverWidget.border": "#1A1A1A",
        "notifications.background": "#383432",
        "notifications.border": "#1A1A1A"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#ebe5d5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "Colors",
            "scope": ["constant.other.color"],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": ["meta.block variable.other"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": ["support.other.variable", "string.other.link"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Sub-methods",
            "scope": [
                "entity.name.module.js",
                "variable.import.parameter.js",
                "variable.other.class.js"
            ],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Language methods",
            "scope": ["variable.language"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": ["entity.name.method.js"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "CSS Classes",
            "scope": ["entity.other.attribute-name.class"],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "CSS ID's",
            "scope": ["source.sass keyword.control"],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Inserted",
            "scope": ["markup.inserted"],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Deleted",
            "scope": ["markup.deleted"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Changed",
            "scope": ["markup.changed"],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "URL",
            "scope": ["*url*", "*link*", "*uri*"],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "ES7 Bind Operator",
            "scope": [
                "source.js constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "JSON Key - Level 1",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#ebe5d5"
            }
        },
        {
            "name": "JSON Key - Level 2",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#e07e3d"
            }
        },
        {
            "name": "JSON Key - Level 3",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#fcfcf0"
            }
        },
        {
            "name": "Plain Text",
            "scope": [
                "text.plain"
            ],
            "settings": {
                "foreground": "#fcfcf0"
            }
        }
    ]
}